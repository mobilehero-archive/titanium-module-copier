{
	"name": "@titanium/module-copier",
	"version": "1.0.13",
	"description": "‚≠ê  Axway Amplify module for copying dependencies to Appcelerator Titanium Resources SDK",
	"main": "index.js",
	"scripts": {
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"lint": "eslint . --fix",
		"prep": "npm install --ignore-scripts && npm run list",
		"test": "npm install --ignore-scripts && npm run prep",
		"prepublishOnly2": "git pull && npm test && bump insane",
		"prepublishOnly": "git pull && npm test && npm audit fix && bump insane"
	},
	"author": {
		"name": "Brenton House",
		"email": "brenton.house@gmail.com",
		"url": "https://brenton.house"
	},
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-module-copier"
	},
	"license": "MIT",
	"devDependencies": {
		"@geek/eslint-config": "1.0.3",
		"eslint": "^8.6.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsdoc": "^37.5.1",
		"eslint-plugin-promise": "^6.0.0"
	},
	"dependencies": {
		"fs-extra": "^10.0.0",
		"lodash": "^4.17.21",
		"make-synchronous": "^1.0.0"
	},
	"bin": {
		"copymodules": "./bin/cli.js"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
